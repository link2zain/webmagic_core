{
  "name": "link2zain/webmagic_core",
  "description": "Package with core laravel-components",
  "license": "MIT",
  "authors": [
    {
      "name": "Zain Ul Abideen",
      "email": "link2zain0@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "7.1 - 7.4",
    "laravel/framework": "5.5 - 7",
    "laracasts/presenter": "^0.2.1"
  },
  "require-dev": {
    "phpunit/phpunit": "6.0 - 9.0"
  },
  "autoload": {
    "psr-4": {
      "Webmagic\\Core\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
      "minimum-stability": "dev",
    "prefer-stable": true,
  "archive": {
    "exclude": [
      "/tests",
      "test.bat",
      "phpunit.xml"
    ]
  }
}
